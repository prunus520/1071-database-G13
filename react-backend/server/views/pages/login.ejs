<!DOCTYPE html>
<html>

<head>
    <% path = '../partials/' %>
    <%- include(path + 'head') %>
</head>

<body>
    <!-- NAVBAR -->
    <%- include(path + 'navbar') %>
    <!-- HEADER -->
    <%- include(path + 'header') %>

    <!-- ACTIONS -->
    <div class="container">
        <h1 class="text-center d-4">Log in</h1>
        <form name="loginForm" id="loginForm">
            <div class="form-group">
                <label for="emailInput">Email</label>
                <input type="text" class="form-control" id="EmailInput" name="email" aria-describedby="EmailHelp"
                    placeholder="Email">
            </div>
            <div class="form-group">
                <label for="passwordInput">Password</label>
                <input type="text" class="form-control" id="passwordInput" name="pwd" placeholder="Password">
            </div>
            <button type="submit" class="btn btn-primary" id="btnSubmit">Submit</button>
        </form>
    </div>

    <!-- JavaScript -->
    <%- include(path + 'JavaScript') %>
    <script src="/js/token.js"></script>
</body>

</html>
